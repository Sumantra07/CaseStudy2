        <!DOCTYPE html>
        <html>
        <head>
            <title><%= title %></title>
            <meta name="viewport" content="width=device-width, intial-scale=1.0">
            <!-- Latest compiled and minified CSS -->
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u"
                crossorigin="anonymous">
        <link rel='stylesheet' href='/stylesheets/style.css' />
            <!-- Latest compiled and minified CSS -->
            <script type="text/javascript" src="./script.js"></script>
        </head>
        <body>
                <nav class="navbar navbar-inverse">
                        <div class="container-fluid">
                        <div class="navbar-header">
                            <a class="navbar-brand" href="#">
                            <%=signedup%>  
                            </a>
                        </div>
                        <ul class="nav navbar-nav navbar-right">
                            <li><a href="/login"><span class="glyphicon glyphicon-log-in"></span> Log-out</a></li>
                        </ul>
                        </div>
                    </nav>
            <div class="container fill">
            <div class="row">
                <div class="col-md-4">
                    <div class="row">
                        <div class="col-md-12" id="ProfileOverview">
                            <h3 id="name"><%= data.Cv[0].profile.name %></h3>
                            <p id="designation"><%= data.Cv[0].profile.designation %></p>
                            <p id="overview"><%= data.Cv[0].profile.overview %></p>
                        </div>
                    </div>
                    <div class="row" id="exentedrow">
                        <div class="col-md-12">

                            <div class="background">
                                <div class="top"> </div>
                                <div class="bottom"></div>
                            </div>
                        </div>
                    </div>


                    <div class="row" id="exentedrow">
                        <div class="col-md-12">
                            <h4 class="banner">Personal Highlights</h4>
                            <!--child nodes to be added programatically -->
                            <ul id="personalDetails">
                                <% var arr = data.Cv[0].profile.Personality; %>                          
                                <% for(var i=0; i < arr.length; i++) {%>
                                        <li><%= arr[i] %></li>
                                    <% } %>
                            </ul>
                        </div>
                    </div>
                    <div class="row" id="exentedrow">
                        <div class="col-md-12">
                            <h4 class="banner">Contact Details</h4>

                            <p class="contactDetails" id="address"><%= data.Cv[0].profile.address %></p>
                            <p class="contactDetails" id="Email"><%= data.Cv[0].profile.Email %></p>
                            <p class="contactDetails" id="mobile"><%= data.Cv[0].profile.Mobile %></p>
                        </div>
                    </div>
                </div>
                <div class="col-md-8">
                    <div class="row" id="informationPanel">
                        <div class="col-md-12">
                            <h3>
                                Basic information
                            </h3>
                            <hr>
                            <p id="information"><%= data.Cv[1].information.BasicInformation %></p>

                        </div>
                    </div>
                    <div class="row" id="informationPanel">
                        <div class="col-md-12">
                            <h3>
                                Education
                            </h3>
                            <hr>
                            <p id="education"><%= data.Cv[1].information.Education %></p>

                        </div>
                    </div>
                    <div class="row" id="informationPanel">
                        <div class="col-md-12">
                            <h3>
                                Profesional Experience
                            </h3>
                            <hr>
                            <!--child nodes to be added-->
                            <div id="companyDetails">
                            <% var arr = data.Cv[1].information.Experience; %>                          
                            <% for(var i=0; i < arr.length; i++) {%>
                                <p><%= arr[i].role %></p>
                                <p><%= arr[i].organization %></p>
                                <p><%= arr[i].duration %></p>
                                <p><%= arr[i].work %></p>
                                <p></p>
                                
                            <% } %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </body>

        </html>
